@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix synbio: <http://example.com/synbio/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

synbio:ModCollection a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Modification Collection" ;
    sh:property [ sh:class synbio:Modification ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path synbio:modifications ] ;
    sh:targetClass synbio:ModCollection .

synbio:OmniCollection a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:class synbio:Ntseq ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path synbio:sequences ],
        [ sh:class synbio:Modification ;
            sh:nodeKind sh:IRI ;
            sh:order 1 ;
            sh:path synbio:modifications ] ;
    sh:targetClass synbio:OmniCollection .

synbio:Organism a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Organism" ;
    sh:property [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path synbio:organism_id ],
        [ sh:maxCount 1 ;
            sh:order 2 ;
            sh:path synbio:details ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path synbio:NCBI_id ] ;
    sh:targetClass synbio:Organism .

synbio:Part a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Part" ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path synbio:bio_safety_level ],
        [ sh:class synbio:Person ;
            sh:maxCount 1 ;
            sh:nodeKind sh:IRI ;
            sh:order 2 ;
            sh:path synbio:creator ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 9 ;
            sh:path synbio:part_name ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 14 ;
            sh:path synbio:summary ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path synbio:alias ],
        [ sh:maxCount 1 ;
            sh:order 7 ;
            sh:path synbio:notes ],
        [ sh:maxCount 1 ;
            sh:order 12 ;
            sh:path synbio:references ],
        [ sh:maxCount 1 ;
            sh:order 4 ;
            sh:path synbio:genotype_phenotype ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 13 ;
            sh:path synbio:status ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path synbio:funding_source ],
        [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path synbio:intellectual_property ],
        [ sh:maxCount 1 ;
            sh:order 6 ;
            sh:path synbio:keywords ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 10 ;
            sh:path synbio:part_type ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 11 ;
            sh:path synbio:principal_investigator ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 8 ;
            sh:path synbio:part_id ] ;
    sh:targetClass synbio:Part .

synbio:SeqCollection a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Sequence Collection" ;
    sh:property [ sh:class synbio:Ntseq ;
            sh:nodeKind sh:IRI ;
            sh:order 0 ;
            sh:path synbio:sequences ] ;
    sh:targetClass synbio:SeqCollection .

synbio:Person a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Person" ;
    sh:property [ sh:maxCount 1 ;
            sh:order 8 ;
            sh:path synbio:is_staff ],
        [ sh:maxCount 1 ;
            sh:order 7 ;
            sh:path synbio:email ],
        [ sh:maxCount 1 ;
            sh:order 4 ;
            sh:path synbio:last_login ],
        [ sh:maxCount 1 ;
            sh:order 9 ;
            sh:path synbio:is_active ],
        [ sh:maxCount 1 ;
            sh:order 6 ;
            sh:path synbio:username ],
        [ sh:maxCount 1 ;
            sh:order 5 ;
            sh:path synbio:is_superuser ],
        [ sh:maxCount 1 ;
            sh:order 0 ;
            sh:path synbio:person_id ],
        [ sh:maxCount 1 ;
            sh:order 3 ;
            sh:path synbio:last_name ],
        [ sh:maxCount 1 ;
            sh:order 1 ;
            sh:path synbio:person_name ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path synbio:first_name ],
        [ sh:maxCount 1 ;
            sh:order 10 ;
            sh:path synbio:date_joined ] ;
    sh:targetClass synbio:Person .

