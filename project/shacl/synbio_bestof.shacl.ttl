synbio:Ntseq a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:name "Nucleotide sequence" ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path synbio:seq_type ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path synbio:seq_id ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path synbio:part_id ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path synbio:seq_name ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path synbio:date_added ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 6 ;
            sh:path synbio:sequence ;
            sh:pattern "^[ACGTacgt]+$" ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path synbio:file ] ;
    sh:targetClass synbio:Ntseq .

